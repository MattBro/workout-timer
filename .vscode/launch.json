{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Timer Demo",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["dev"],
      "cwd": "${workspaceFolder}/apps/demo",
      "env": {
        "PATH": "${env:PNPM_HOME}:${env:NVM_BIN}:${env:HOME}/.asdf/shims:${env:HOME}/.volta/bin:/opt/homebrew/bin:/usr/local/bin:${env:PATH}"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Run All Dev",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["dev"],
      "cwd": "${workspaceFolder}",
      "env": {
        "PATH": "${env:PNPM_HOME}:${env:NVM_BIN}:${env:HOME}/.asdf/shims:${env:HOME}/.volta/bin:/opt/homebrew/bin:/usr/local/bin:${env:PATH}"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Build Timer",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["build"],
      "cwd": "${workspaceFolder}",
      "env": {
        "PATH": "${env:PNPM_HOME}:${env:NVM_BIN}:${env:HOME}/.asdf/shims:${env:HOME}/.volta/bin:/opt/homebrew/bin:/usr/local/bin:${env:PATH}"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Run Tests",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["test"],
      "cwd": "${workspaceFolder}",
      "env": {
        "PATH": "${env:PNPM_HOME}:${env:NVM_BIN}:${env:HOME}/.asdf/shims:${env:HOME}/.volta/bin:/opt/homebrew/bin:/usr/local/bin:${env:PATH}"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
